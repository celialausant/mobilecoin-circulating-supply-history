	<!-- this script is for displaying and hiding the mobile navigation. Note that it does nothing useful in desktop mode. -->
	<script>

		var mcfMobileNavController = {
			button: null,
			container: null,
			buttonIconContainer: null,
			getNodes: function() {
				this.button = document.getElementById("mobile-nav-toggle");
				this.container = document.getElementById("mobile-nav-container");
				this.buttonIconContainer = document.getElementById("mobile-nav-icon-container");
			},
			showMobileNav: function() {
				if (! this.container.classList.contains("visible")) {
					this.container.classList.add("visible");
					this.button.classList.add("toggled");

					this.buttonIconContainer.classList.add("top-0");
					this.buttonIconContainer.classList.remove("-top-full");
				}
			},
			hideMobileNav: function() {
				this.container.classList.remove("visible");
				this.button.classList.remove("toggled");
				
				this.buttonIconContainer.classList.add("-top-full");
				this.buttonIconContainer.classList.remove("top-0");
				
			},
			toggleMobileNav: function() {
				if (this.container.classList.contains("visible")) {
					this.hideMobileNav();
				}
				else {
					this.showMobileNav();
				}
			}
		};

		document.addEventListener("DOMContentLoaded", function(event) {
			mcfMobileNavController.getNodes();

			mcfMobileNavController.button.addEventListener("click", function() { mcfMobileNavController.toggleMobileNav(); });
		});

	</script>

